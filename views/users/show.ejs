<!-- Project 2: Dance Portfolio -->
<!-- View individual user page -->

<!DOCTYPE html>
<html>
  <head>
    <% include ../partials/head %>
  </head>
  <body>
    <header>
      <% include ../partials/nav %>
    </header>
    <main>
      <h1><%=user.displayName%>'s Posting</h1>


            <div class="container">
              <% for(var i = 0; i < dances.length; i++){ %>
              <div class="dance">
                <h3 class="dance-title"><a href="/dances/<%=dances[i].id%>"><%=dances[i].title%></a></h3>
                <div class="danceImg">
                  <img class="danceImg" src="<%=dances[i].img%>" alt="">
                </div>
              </div>
              <% } %>
            </div>


    </main>
    <footer>

    </footer>
  </body>
</html>
